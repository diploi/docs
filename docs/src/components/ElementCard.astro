---
import type { DiploiElement } from "./types";

interface Props {
  element: DiploiElement;
}

const { element } = Astro.props;
---

<div class="flex gap-2">
  <img
    src={`${element.url}/blob/main/.diploi/icon.svg?raw=true`}
    alt={element.name}
    class="element-icon"
    width={28}
    height={28}
  />
  <strong>{element.name}</strong>
</div>
